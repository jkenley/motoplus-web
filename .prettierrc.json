{
    "importOrder": [
        "<BUILTIN_MODULES>",
        "<THIRD_PARTY_MODULES>",
        "",
        "^@/(.*)$",
        "^[./]",
        "",
        "<TYPES>",
        "<TYPES>^[./]"
    ],
    "plugins": [
        "prettier-plugin-sort-json",
        "@ianvs/prettier-plugin-sort-imports"
    ],
    "endOfLine": "lf",
    "printWidth": 120,
    "tabWidth": 2,
    "semi": true,
    "useTabs": false,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always"
}
